{
  "version": "1.0",
  "defaultProvider": "unpkg",
  "libraries": [
    {
      "provider": "cdnjs",
      "library": "jquery@3.3.1",
      "destination": "wwwroot/jquery/"
    },
    {
      "provider": "cdnjs",
      "library": "twitter-bootstrap@4.1.0",
      "destination": "wwwroot/twitter-bootstrap/"
    },
    {
      "provider": "cdnjs",
      "library": "twitter-bootstrap@4.1.3",
      "destination": "wwwroot/twitter-bootstrap/",
      "files": [
        "js/bootstrap.min.js",
        "css/bootstrap.min.css"
      ]
    },
    {
      "library": "@aspnet/signalr@1.0.3",
      "destination": "wwwroot/@aspnet/signalr/",
      "files": [
        "dist/browser/signalr.min.js"
      ]
    },
    {
      "library": "@microsoft/signalr@latest",
      "destination": "wwwroot/@microsoft/signalr/",
      "files": [
        "dist/browser/signalr.js",
        "dist/browser/signalr.min.js"
      ]
    }
  ]
}